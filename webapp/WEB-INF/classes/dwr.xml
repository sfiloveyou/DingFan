<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://getahead.org/dwr/dwr20.dtd">
<dwr>
	<allow>

		<create javascript="systemService" creator="spring">
			<param name="beanName" value="dwrService"></param>
		</create>
		
		<convert match="DB.BusinessUnit" converter="bean" javascript="BusinessUnit">
		</convert>
		<convert match="DB.User" converter="bean" javascript="User">
		</convert>
	</allow>
</dwr>

